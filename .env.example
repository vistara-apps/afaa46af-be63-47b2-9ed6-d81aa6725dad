# SquadSats Environment Configuration
# Copy this file to .env.local and fill in your values

# Coinbase OnchainKit API Key
# Get this from: https://portal.cdp.coinbase.com/
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_coinbase_onchainkit_api_key_here

# Base Network RPC URL (optional - defaults to public RPC)
NEXT_PUBLIC_BASE_RPC_URL=https://mainnet.base.org

# Smart Contract Addresses (to be filled after deployment)
SQUAD_SATS_FUND_ADDRESS=0x0000000000000000000000000000000000000000

# Optional: Farcaster configuration
FARCASTER_CLIENT_ID=your_farcaster_client_id
FARCASTER_CLIENT_SECRET=your_farcaster_client_secret

# Optional: Analytics
NEXT_PUBLIC_ANALYTICS_ID=your_analytics_id

